}




function updateRequest(wf,request_id){
	var gr = new GlideRecord('x_isgi_rr_request');
	gr.addQuery('sys_id',request_id);
	gr.query();
	if(gr.next()){
		gr.workflow_state=wf;
		gr.update();
	}
